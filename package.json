{
  "name": "jspdf-barcode",
  "version": "0.1.0",
  "description": "barcode generator plugin for jspdf",
  "main": "dist/jspdf-barcode.js",
  "types": "types.d.ts",
  "exports": {
    "import": "./dist/jspdf-barcode.js",
    "require": "./dist/jspdf-barcode.cjs"
  },
  "workspaces": {"packages": ["example/*"]},
  "scripts": {
    "build": "webpack --mode production",
    "build-example": "cd example && npm ci && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/castrix/jspdf-barcode/blob/main/LICENSE"
  },
  "keywords": [
    "jspdf",
    "barcode",
    "barcode",
    "generator",
    "pdf"
  ],
  "author": "ihsan fajar ramadhan",
  "license": "MIT",
  "dependencies": {
    "code-128-encoder": "^3.1.1",
    "jspdf": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.2.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
